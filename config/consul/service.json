{
    "services": [
    {
        "id": "Gate1",
        "name": "Gate",
        "tags": ["Gate"],
        "address": "192.168.1.137",
        "port": 80,
        "check": {
          "id": "http_check_gate1",
          "name": "HTTP API on port 80",
          "http": "http://192.168.1.137:80/health_check",
          "method": "GET",
          "interval": "10s",
          "timeout": "3s"
        }
    },

    {
        "id": "Gate2",
        "name": "Gate",
        "tags": ["Gate"],
        "address": "192.168.1.138",
        "port": 80,
        "check": {
          "id": "http_check_gate2",
          "name": "HTTP API on port 80",
          "http": "http://192.168.1.138:80/health_check",
          "method": "GET",
          "interval": "10s",
          "timeout": "3s"
        }
    }
    ],

    "watches": [
    {
        "type": "key",
        "key": "service_config",
        "handler_type": "http",
        "http_handler_config": {
            "path": "http://192.168.1.137:80/watch_key",
            "method": "GET",
            "timeout": "10s",
            "tls_skip_verify": false
        }
    },

    {
        "type": "service",
        "service": "Gate",
        "passingonly": true,
        "handler_type": "http",
        "http_handler_config": {
            "path": "http://192.168.1.137:80/watch_service",
            "method": "GET",
            "timeout": "10s",
            "tls_skip_verify": false
        }
    }

  ]
}
